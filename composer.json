{
  "name": "mgoigfer/nova-spotify-auth-tool",
  "description": "A Laravel Nova tool.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Mgoigfer\\NovaSpotifyAuthTool\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mgoigfer\\NovaSpotifyAuthTool\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
