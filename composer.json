{
  "name": "mgoigfer/nova-spotify-auth-tool",
  "description": "A Spotify auth tool to use in your Laravel Nova apps.",
  "keywords": [
    "laravel",
    "nova",
    "spotify"
  ],
  "authors": [
    {
      "name": "Mikel Goig",
      "email": "m@mikelgoig.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "jwilsson/spotify-web-api-php": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Mgoigfer\\NovaSpotifyAuthTool\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mgoigfer\\NovaSpotifyAuthTool\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
